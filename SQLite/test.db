CREATE TABLE USER (ID CHAR NOT NULL PRIMARY KEY, TITLE TEXT NOT NULL, TEXT TEXT NOT NULL);
INSERT INTO USER (ID, TITLE, TEXT) VALUES ('7', '生命中最重要的事', '生命中最重要的肯定是陪伴自己的家人生，這樣的人生才會更精彩。在成功之前，不要抱怨命運的安排，但不要抱怨命運的殘酷，即使失敗是自己最好的導演，也要在挫折的陰影下一個人生。
');

COMMIT TRANSACTION;
PRAGMA foreign_keys = on;
